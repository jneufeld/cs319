@model DREAM.Models.DropDown

@{
    ViewBag.Title = "Edit ";
}

<h2>Edit</h2>


@using (Html.BeginForm()) {
    @Html.AntiForgeryToken()
    @Html.ValidationSummary(true)
    @Html.HiddenFor(d => Model.ID)

    <table>
        <tr>
            <td>            
                @Html.LabelFor(d => Model.Code)
            </td>
            <td>
                @Html.EditorFor(d => Model.Code)
                @Html.ValidationMessageFor(d => Model.Code)
            </td>
        </tr>
        <tr><td>            
                @Html.LabelFor(d => Model.FullName)
            </td>
            <td>
                @Html.EditorFor(d => Model.FullName)
                @Html.ValidationMessageFor(d => Model.FullName)
            </td>
        <tr>
            <td>
                <button type="submit" class="btn span2">Submit</button>
            </td>
        </tr>
    </table>
}
